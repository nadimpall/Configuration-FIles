pipeline{
    agent any
    environment{
        PATH = "E:/MAVEN/apache-maven-3.5.4/bin:$PATH"
    }
    stages{
        stage("Git checkout"){
            steps{
                git 'https://github.com/nadimpall/karthikproject.git'
                echo "****************Successfully cloned****************"
            }
        }
       
       stage("Build"){
           steps{
                bat "mvn clean install"
                echo "****************Successfully Build*****************"
           }    
       } 
    }
}
